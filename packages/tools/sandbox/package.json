{
    "name": "@tools/sandbox",
    "private": true,
    "version": "1.0.0",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "scripts": {
        "build": "npm run clean && npm run compile",
        "build:declaration": "build-tools -c pud --config ./config.json",
        "build:deployment": "npm run clean && webpack --env mode=production",
        "compile": "tsc -b tsconfig.build.json",
        "postcompile": "build-tools -c pa",
        "serve": "webpack serve",
        "serve:prod": "webpack serve --env mode=production",
        "serve:https": "webpack serve --env mode=development --server-type https --host ::",
        "serve:dev": "webpack serve --env mode=development --env watch=all",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "draco3dgltf": "^1.5.6",
        "meshoptimizer": "^0.20.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "tweakpane": "^4.0.3"
    },
    "devDependencies": {
        "@dev/build-tools": "1.0.0",
        "@dev/core": "1.0.0",
        "@dev/loaders": "1.0.0",
        "@svgr/webpack": "^7.0.0",
        "@tweakpane/core": "^2.0.3",
        "@types/dagre": "^0.7.47",
        "@types/draco3dgltf": "^1.4.3",
        "@types/react": "^17.0.30",
        "@types/react-dom": "^17.0.10",
        "assert": "^2.1.0",
        "buffer": "^6.0.3",
        "copy-webpack-plugin": "^11.0.0",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.4.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.4.0",
        "https-browserify": "^1.0.0",
        "mini-css-extract-plugin": "^2.4.3",
        "os-browserify": "^0.3.0",
        "process": "^0.11.10",
        "sass-loader": "^13.0.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "style-loader": "^3.3.0",
        "url": "^0.11.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.73.0",
        "webpack-cli": "^5.1.0",
        "webpack-merge": "^5.8.0"
    },
    "sideEffects": true
}
